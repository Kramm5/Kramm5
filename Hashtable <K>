import java.util.Hashtable;

public class HashTableExample {
    public static void main(String[] args) {
        // Creating a Hashtable
        Hashtable<String, Integer> hashtable = new Hashtable<>();

        // Adding key-value pairs to the Hashtable
        hashtable.put("Mark", 18);
        hashtable.put("Dreen", 14);
        hashtable.put("Jacob", 9);
        
       
        // Accessing values
        System.out.println("Age of Brother: " + hashtable.get("Bob"));

        // Iterating over the Hashtable
        for (String name : hashtable.keySet()) {
            System.out.println("Name: " + name + ", Age: " + hashtable.get(name));
        }
    }
}
